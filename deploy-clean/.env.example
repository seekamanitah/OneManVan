# Copy to .env on the server (do not commit .env)

# Web UI exposed port on the server
WEBUI_PORT=7159

# SQL Server exposed port on the server
DB_PORT=1433

# Required: SQL Server SA password
SA_PASSWORD=CHANGE_ME_STRONG_SA_PASSWORD

# Optional: SQL Server Edition
MSSQL_PID=Express

# ASP.NET environment
ASPNETCORE_ENVIRONMENT=Production

# Database names
APP_DB_NAME=TradeFlowFSM
IDENTITY_DB_NAME=TradeFlowIdentity

# Admin seeding (optional but recommended for unattended first run)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=CHANGE_ME_STRONG_ADMIN_PASSWORD
