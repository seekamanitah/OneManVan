# Copy to .env on the server (do not commit .env)

# Web UI exposed port on the server (default 5000 for seekmedia.duckdns.org)
WEBUI_PORT=5000

# SQL Server exposed port on the server (mobile app connects on this port)
DB_PORT=1433

# Required: SQL Server SA password
SA_PASSWORD=CHANGE_ME_STRONG_SA_PASSWORD

# Optional: SQL Server Edition
MSSQL_PID=Express

# ASP.NET environment
ASPNETCORE_ENVIRONMENT=Production

# Database names
APP_DB_NAME=OneManVanDB
IDENTITY_DB_NAME=OneManVanIdentity

# Admin seeding (optional but recommended for unattended first run)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=CHANGE_ME_STRONG_ADMIN_PASSWORD

# Mobile sync user (for mobile app SQL Server connection)
SYNC_USER=onemanvan_sync
SYNC_PASSWORD=CHANGE_ME_SYNC_PASSWORD
